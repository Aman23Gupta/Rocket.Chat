Object\n      });\n    });\n  }\n});","map":{"version":3,"sources":["client/methods/updateMessage.js"],"names":["Meteor","module","link","v","TimeSync","Tracker","moment","_","hasAtLeastOnePermission","ChatMessage","settings","t","callbacks","dispatchToastMessage","methods","updateMessage","message","userId","originalMessage","findOne","_id","hasPermission","rid","editAllowed","get","editOwn","msg","u","me","users","type","action","blockEditInMinutes","isNumber","ts","msgTs","currentTsDiff","diff","nonreactive","isNaN","serverOffset","editedAt","Date","now","editedBy","username","run","messageObject","attachments","description","undefined","update","$set"],"mappings":"AAAA,IAAIA,MAAJ;AAAWC,MAAM,CAACC,IAAP,CAAY,eAAZ,EAA4B;AAACF,EAAAA,MAAM,EAAC,UAASG,CAAT,EAAW;AAACH,IAAAA,MAAM,GAACG,CAAP;AAAS;AAA7B,CAA5B,EAA2D,CAA3D;AAA8D,IAAIC,QAAJ;AAAaH,MAAM,CAACC,IAAP,CAAY,wBAAZ,EAAqC;AAACE,EAAAA,QAAQ,EAAC,UAASD,CAAT,EAAW;AAACC,IAAAA,QAAQ,GAACD,CAAT;AAAW;AAAjC,CAArC,EAAwE,CAAxE;AAA2E,IAAIE,OAAJ;AAAYJ,MAAM,CAACC,IAAP,CAAY,gBAAZ,EAA6B;AAACG,EAAAA,OAAO,EAAC,UAASF,CAAT,EAAW;AAACE,IAAAA,OAAO,GAACF,CAAR;AAAU;AAA/B,CAA7B,EAA8D,CAA9D;AAAiE,IAAIG,MAAJ;AAAWL,MAAM,CAACC,IAAP,CAAY,QAAZ,EAAqB;AAAC,aAAQ,UAASC,CAAT,EAAW;AAACG,IAAAA,MAAM,GAACH,CAAP;AAAS;AAA9B,CAArB,EAAqD,CAArD;;AAAwD,IAAII,CAAJ;;AAAMN,MAAM,CAACC,IAAP,CAAY,YAAZ,EAAyB;AAAC,aAAQ,UAASC,CAAT,EAAW;AAACI,IAAAA,CAAC,GAACJ,CAAF;AAAI;AAAzB,CAAzB,EAAoD,CAApD;AAAuD,IAAIK,uBAAJ;AAA4BP,MAAM,CAACC,IAAP,CAAY,gCAAZ,EAA6C;AAACM,EAAAA,uBAAuB,EAAC,UAASL,CAAT,EAAW;AAACK,IAAAA,uBAAuB,GAACL,CAAxB;AAA0B;AAA/D,CAA7C,EAA8G,CAA9G;AAAiH,IAAIM,WAAJ;AAAgBR,MAAM,CAACC,IAAP,CAAY,yBAAZ,EAAsC;AAACO,EAAAA,WAAW,EAAC,UAASN,CAAT,EAAW;AAACM,IAAAA,WAAW,GAACN,CAAZ;AAAc;AAAvC,CAAtC,EAA+E,CAA/E;AAAkF,IAAIO,QAAJ;AAAaT,MAAM,CAACC,IAAP,CAAY,2BAAZ,EAAwC;AAACQ,EAAAA,QAAQ,EAAC,UAASP,CAAT,EAAW;AAACO,IAAAA,QAAQ,GAACP,CAAT;AAAW;AAAjC,CAAxC,EAA2E,CAA3E;AAA8E,IAAIQ,CAAJ;AAAMV,MAAM,CAACC,IAAP,CAAY,wBAAZ,EAAqC;AAACS,EAAAA,CAAC,EAAC,UAASR,CAAT,EAAW;AAACQ,IAAAA,CAAC,GAACR,CAAF;AAAI;AAAnB